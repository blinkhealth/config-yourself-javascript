{
  "private": false,
  "version": "1.0.0",
  "name": "@blinkhealth/config-yourself",
  "description": "A typescrypt + javascript runtime for decrypting config-yourself files",
  "homepage": "https://github.com/blinkhealth/config-yourself-typescript",
  "bugs": {
    "url": "https://github.com/blinkhealth/config-yourself-typescript/issues"
  },
  "author": {
    "name": "Blink Health Engineering",
    "url": "https://github.com/blinkhealth"
  },
  "repository": "github:blinkhealth/config-yourself-typescript",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "src": "src/index.ts",
  "types": "dist/types/index.d.ts",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10.9.0"
  },
  "scripts": {
    "build": "rollup -c",
    "docs": "typedoc --out docs src",
    "test": "jest",
    "test:coverage": "yarn test --ci --coverage --reporters=default --reporters=jest-junit",
    "lint": "yarn prettier './src/**/*.ts'",
    "lint:fix": "yarn lint --write",
    "lint:check": "yarn lint --check"
  },
  "dependencies": {
    "@aws-sdk/client-kms-node": "0.1.0-preview.8",
    "deepmerge": "4.2.2",
    "js-yaml": "3.13.1",
    "openpgp": "4.6.2"
  },
  "devDependencies": {
    "@types/jest": "24.0.22",
    "@types/js-yaml": "3.12.1",
    "@types/mock-fs": "4.10.0",
    "@types/node": "12.12.7",
    "@types/openpgp": "4.4.7",
    "@types/scrypt": "6.0.0",
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "@typescript-eslint/parser": "2.6.1",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-prettier": "3.1.1",
    "jest": "24.9.0",
    "jest-junit": "9.0.0",
    "mock-fs": "4.10.3",
    "prettier": "1.19.1",
    "rollup": "1.26.4",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-multi-entry": "2.1.0",
    "rollup-plugin-typescript2": "0.25.2",
    "ts-jest": "24.1.0",
    "ts-loader": "6.2.1",
    "typedoc": "0.15.0",
    "typescript": "3.7.2"
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "includeConsoleOutput": true,
    "classNameTemplate": "{classname}",
    "titleTemplate": "{classname}: {title}"
  }
}
